package CoreJava;

public class ArrayPractise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a1[][]= new int[2][3];
		a1[0][0]=2;
		a1[0][1]=3;
		a1[0][2]=4;
		a1[1][0]=5;
		a1[1][1]=6;
		a1[1][2]=7;
		
		//System.out.println(a1.length);
		
		int a2[][]= {{2,4,6},{3,5,7}};
		for(int i=0;i<=a1.length;i++) 
		{
			for(int j=0;j<=2;j++) {
			System.out.println(a1[i][j]);
			}
		}
	}

}
