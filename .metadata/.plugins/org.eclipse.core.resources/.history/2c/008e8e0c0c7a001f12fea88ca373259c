package CoreJava;

public class Childvehicle extends ParentVehicle {
	
	public void colour() {
		System.out.println(color);
	}
	
	String color="blue";

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Childvehicle c1=new Childvehicle();
		c1.audiosystem();
		c1.brakes();
		c1.colour();
		System.out.println(c1.colour);
	}

}
